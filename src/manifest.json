{
  "name": "Repro Web SDK Viewer",
  "version": "1.0",
  "description": "DEBUG!",
  "browser_action": {
    "default_icon": {
      "16": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    },
    "default_title": "Repro Devtools",
    "default_popup": "not_detected.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "detector.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*",
    "activeTab"
  ],
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "manifest_version": 2
}
